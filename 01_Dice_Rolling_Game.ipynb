    "import random\n",
    "while True:\n",
    "    roll=input(\"Roll the dice? (y/n):\").lower()\n",
    "    if roll==\"n\":\n",
    "        print(\"Thanks for playing\")\n",
    "        break\n",
    "    elif roll==\"y\":\n",
