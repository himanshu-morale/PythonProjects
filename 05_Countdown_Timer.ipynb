   "source": [
    "import time\n",
    "my_time=int(input(\"Enter time in seconds: \"))\n",
    "for i in range(my_time,0,-1):\n",
    "    seconds=i%60\n",
    "    minute=int(i/60)%60\n",
    "    hours=int(i/3600)\n",
    "    print(f\"{hours:02}:{minute:02}:{seconds:02}\")\n",
    "    time.sleep(1)\n",
    "print(\"TIME'S UP\")    "
   ]
  }
